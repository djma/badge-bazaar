  "main": "index.js",
  "_scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "yarn tsc && yarn prisma:generate && esbuild src/web/index.tsx --bundle --minify --outdir=www/dist --loader:.svg=dataurl",
    "start": "node dist/src/server.js",
    "prisma:generate": "prisma generate"
  },
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server.ts",
    "#dev:client": "esbuild src/web/index.tsx --bundle --outdir=www/dist --watch",
    "dev:client": "ts-node-dev build.ts --watch",
    "build": "yarn prisma:generate && ts-node build.ts",
    "start": "node dist/src/server.js",
    "prisma:generate": "prisma generate",
    "clean": "rm -rf dist && rm -rf www/dist"
  },
